datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model CompDataSegmentItem {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  name      String?
  updatedAt DateTime @updatedAt
  value     String?
}

model Item {
  b31CommercialGoods String?
  b33CombNomenCode   String?
  b33TaricNomenCode  String?
  b34OriginCountry   String?
  b37CatType         String?
  b37PreviousPro     String?
  b37RequestedPro    String?
  b37Subcat          String?
  b38NetMass         Int?
  b41UnitNum         Int?
  b42ItemPrice       Int?
  createdAt          DateTime @default(now())
  id                 String   @id @default(cuid())
  itemNum            Int?
  updatedAt          DateTime @updatedAt
}

model Consignor {
  businessaddr   String?
  businessname   String?
  businessnation String?
  createdAt      DateTime @default(now())
  id             String   @id @default(cuid())
  registered     Boolean?
  updatedAt      DateTime @updatedAt
}

model ImportHeader {
  action            String?
  b1DeclaSub1       String?
  b20DelivTermsSub1 String?
  b20DelivTermsSub2 String?
  b20DelivTermsSub3 String?
  b22CurrencyCode   String?
  b22TotalAmount    Int?
  b25BorderTrans    Int?
  b26InlandTrans    Int?
  b27PlaceOfLoading String?
  b48DeferredPay    Int?
  b5Items           Int?
  b9TotalFreight    Int?
  b9TotalInsurance  Int?
  box47CdPayMethod  String?
  boxaOfficeCode    String?
  boxaOfficeSubCode String?
  cdGuaranteeMethod String?
  createdAt         DateTime @default(now())
  direction         String?
  id                String   @id @default(cuid())
  information       String?
  moduleField       String?
  updatedAt         DateTime @updatedAt
  userId            String?
}

model Consignee {
  createdAt  DateTime @default(now())
  id         String   @id @default(cuid())
  registered Boolean?
  trnumber   String?
  updatedAt  DateTime @updatedAt
}
